{
  "name": "dungeonGen",
  "version": "0.0.1",
  "engines": {
    "node": "4.x.x"
  },
  "description": "Generate a dungeon Map",
  "main": "index.js",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "istanbul": "^0.4.1",
    "jscs": "^2.7.0",
    "jshint": "^2.8.0",
    "tape": "^4.2.2",
    "watch": "^0.16.0"
  },
  "scripts": {
    "test": "tape test/**/*.js |tap-dot",
    "watch": "watch 'npm test' -d",
    "circle_ci_test": "istanbul cover --dir $CIRCLE_ARTIFACTS tape test/**/*.js",
    "cover": "istanbul cover tape test/**/*.js |tap-spec",
    "lint": "jscs lib/**/*.js test/**/*.js ; jshint lib/**/*.js test/**/*.js || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SachaCR/dungeonGen.git"
  },
  "keywords": [
    "Dungeon",
    "Generator",
    "RPG",
    "Map"
  ],
  "author": "SachaCR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SachaCR/dungeonGen/issues"
  },
  "homepage": "https://github.com/SachaCR/dungeonGen"
}
